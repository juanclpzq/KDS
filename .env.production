# Production Environment Configuration
# For live deployment

# Backend API URL (Update with actual production URL)
VITE_API_URL=https://api.lamarque.mx

# KDS Location ID (Update with actual location UUID)
VITE_KDS_LOCATION_ID=<actual-location-uuid>

# KDS Device ID (Update with actual device number)
VITE_DEVICE_ID=1

# Disable demo mode in production
VITE_DEMO_MODE=false

# Always use real backend in production
VITE_USE_REAL_BACKEND=true

# Optional: provide an auth token or bypass token if the backend requires it
# VITE_BYPASS_TOKEN=your-bypass-token-here
